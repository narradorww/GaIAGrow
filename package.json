    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1 ",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "^8.0.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.5",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3"
  }
