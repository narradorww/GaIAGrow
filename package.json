{
  "name": "expo-template-suite-senior",
  "version": "1.1.0",
  "keywords": [
    "expo",
    "typescript",
    "template",
    "expo-template",
    "react-native",
    "react-navigation",
    "jest",
    "eslint",
    "prettier",
    "testing-library"
  ],
  "homepage": "https://github.com/narradorww",
  "author": "Rodrigo Alexandre <https://github.com/narradorww>",
  "license": "GPL-3.0-only",
  "main": "index.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "ci": "yarn lint:fix && yarn type-check && yarn test",
    "prepare": "husky install",
    "release": "standard-version",
    "commit": "npx git-cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
