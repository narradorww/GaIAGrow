    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"]
    },
    "types": ["jest"],
    "allowJs": true,
    "resolveJsonModule": true,
    "moduleResolution": "node16",
    "module": "Node16"
  },
  "include": ["src", "src/types"]
}